{
    "$schema": "./node_modules/nx/schemas/nx-schema.json",
    "tasksRunnerOptions": {
        "default": {
            "runner": "nx/tasks-runners/default",
            "options": {
                "cacheableOperations": [
                    "build"
                ]
            }
        }
    },
    "targetDefaults": {
        "dev": {
            "dependsOn": [
                "^build"
            ]
        }
    },
    "plugins": [
        {
            "plugin": "@nx/eslint/plugin",
            "options": {
                "targetName": "lint"
            }
        }
    ]
}
